{
    "extends": ["react-app", "react-app/jest"],
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "warn",
        "prefer-const": "error",
        "import/order": [
            "error",
            {
                "groups": [
                    "builtin",
                    "external",
                    "internal",
                    "parent",
                    "sibling",
                    "index",
                    "object",
                    "type"
                ],
                "newlines-between": "always-and-inside-groups",
                "warnOnUnassignedImports": true
            }
        ]
    }
}
